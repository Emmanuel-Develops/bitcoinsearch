{
    "env": {
        "browser": true,
        "es2021": true
    },

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "warn",
        "no-console": [
            "warn", { "allow": ["warn", "error"] }
        ]
    }
}
